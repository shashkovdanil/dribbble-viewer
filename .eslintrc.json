{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "globals": {
        "fetch": true
    },
    "rules": {
        "react/jsx-filename-extension": 0,
        "import/prefer-default-export": 0,
        "jsx-a11y/accessible-emoji": 0
    }
}